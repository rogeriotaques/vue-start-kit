{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-typescript"
  ],
  "plugins": [
    "@babel/plugin-transform-runtime",
    [
      "module-resolver",
      {
        "root": [
          "./src",
          "./node_modules/seed-vue/dist"
        ],
        "alias": {
          "~": "./src"
        }
      }
    ]
  ]
}
