{
  "name": "vue-start-kit",
  "version": "1.3.1",
  "description": "A boring Vue start kit made with interesting packages.",
  "main": "dist/index.js",
  "scripts": {
    "start": "parcel src/index.pug --open --no-cache",
    "build": "NODE_ENV=production parcel build src/index.pug --cache-dir=./dist/.cache",
    "test": "TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' mocha --require ts-node/register test/**/*.test.ts"
  },
  "author": "Rogerio Taques",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.21.1",
    "babel-polyfill": "^6.26.0",
    "eva-icons": "^1.1.1",
    "seed-css": "^2.4.0",
    "vue": "^2.6.10",
    "vue-hot-reload-api": "^2.3.3",
    "vue-router": "^3.1.3",
    "vuex": "^3.1.1"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.7.4",
    "@vue/component-compiler-utils": "^2.6.0",
    "babel-core": "^6.26.3",
    "mocha": "^5.2.0",
    "parcel-bundler": "^1.12.3",
    "pug": "^2.0.4",
    "sass": "^1.22.10",
    "ts-node": "^8.3.0",
    "typescript": "^3.6.2",
    "vue-template-compiler": "^2.6.10"
  },
  "alias": {
    "vue": "./node_modules/vue/dist/vue.esm.js"
  }
}
