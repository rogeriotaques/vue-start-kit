{
  "name": "vue-start-kit",
  "version": "2.0.0",
  "description": "Just another starter-kit ðŸ˜… to bootstrap Vue applications.",
  "main": "dist/index.html",
  "scripts": {
    "start": "parcel src/index.pug --no-cache --open",
    "build": "yarn b:cln && yarn b:run && yarn b:cfg",
    "lint": "eslint ./src --ext .ts,.js --no-error-on-unmatched-pattern",
    "unit": "jest",
    "unit:coverage": "jest --collect-coverage",
    "b:cln": "rm -rf dist/",
    "b:run": "NODE_ENV=production parcel build src/index.pug --cache-dir=./dist/.cache",
    "b:cfg": "cp src/.htaccess.sample dist/.htaccess"
  },
  "repository": "git@github.com:rogeriotaques/vue-start-kit.git",
  "author": "Rogerio Taques",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.21.0",
    "seed-vue": "^0.1.3",
    "vue": "^2.6.12",
    "vue-router": "^3.4.9",
    "vuex": "^3.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-0",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.7",
    "@babel/preset-typescript": "^7.12.7",
    "@types/jest": "^26.0.16",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "@vue/component-compiler-utils": "^3.2.0",
    "@vue/composition-api": "^1.0.0-beta.20",
    "@vue/test-utils": "^1.1.2",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-module-resolver": "^4.0.0",
    "browserslist": "^4.15.0",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-vue": "^7.1.0",
    "jest": "^26.6.3",
    "jest-vue-preprocessor": "^1.7.1",
    "parcel-bundler": "^1.12.4",
    "pug": "^3.0.1",
    "sass": "^1.29.0",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.0.0",
    "typescript": "^4.1.2",
    "vue-hot-reload-api": "^2.3.4",
    "vue-jest": "^3.0.7",
    "vue-template-compiler": "^2.6.12"
  },
  "alias": {
    "vue": "./node_modules/vue/dist/vue.esm.js"
  }
}
